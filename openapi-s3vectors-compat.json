        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "vectorBucketName",
                  "indexName",
                  "dimension",
                  "distanceMetric"
                ],
                "properties": {
                  "vectorBucketName": {
                    "type": "string"
                  },
                  "indexName": {
                    "type": "string"
                  },
                  "dimension": {
                    "type": "integer",
                    "minimum": 1,
                    "maximum": 4096
                  },
                  "distanceMetric": {
                    "type": "string",
                    "enum": [
                      "euclidean",
                      "cosine"
                    ]
                  },
                  "dataType": {
                    "type": "string",
                    "enum": [
                      "float32"
                    ],
                    "default": "float32"
                  },
                  "metadataConfiguration": {
                    "type": "object",
                    "properties": {
                      "nonFilterableMetadataKeys": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      }
                    },
                    "additionalProperties": false
                  }
                },
                "additionalProperties": false
              }
            }
          }
        },
